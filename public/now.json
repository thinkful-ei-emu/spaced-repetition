{
  "version": 2,
  "alias": "mac-spaced-repetition",
  "name": "spaced-repetition",
  "routes": [
      {
        "src": "^/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": ".*",
        "dest": "/index.html"
      },
      { 
        "src": "^/favicon.ico$",
        "dest": "/favicon.ico" 
      }
  ]
}